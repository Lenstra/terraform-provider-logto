{
	"datasources": [
		{
			"name": "application",
			"schema": {
				"attributes": [
					{
						"name": "name",
						"string": {
							"computed_optional_required": "computed",
							"description": "The name of the application."
						}
					}
				]
			}
		}
	],
	"provider": {
		"name": "logto"
	},
	"resources": [
		{
			"name": "application",
			"schema": {
				"attributes": [
					{
						"name": "description",
						"string": {
							"computed_optional_required": "computed_optional",
							"description": "The description of the application."
						}
					},
					{
						"name": "name",
						"string": {
							"computed_optional_required": "required",
							"description": "The name of the application.",
							"validators": [
								{
									"custom": {
										"imports": [
											{
												"path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
											}
										],
										"schema_definition": "stringvalidator.LengthBetween(1, 256)"
									}
								}
							]
						}
					},
					{
						"name": "type",
						"string": {
							 "computed_optional_required": "required",
							 "description": "The type of the application that can take these values : Native, SPA, Traditional, MachineToMachine, Protected, SAML.",
							 "plan_modifiers": [
									 {
											 "custom": {
													 "imports": [
															{
																"path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
															}
													 ],
													 "schema_definition": "stringplanmodifier.RequiresReplace()"
											 }
									 }
							 ],
							 "validators": [
									 {
											 "custom": {
													 "imports": [
															 {
																	 "path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
															 }
													 ],
													 "schema_definition": "stringvalidator.OneOf(\"Native\",\"SPA\",\"Traditional\",\"MachineToMachine\",\"Protected\",\"SAML\")"
											 }
									 }
							 ]
						}
					},
					{
						"name": "id",
						"string": {
							"computed_optional_required": "computed",
							"description": "The unique identifier of the application."
						}
					},
					{
						"name": "tenant_id",
						"string": {
							"computed_optional_required": "computed"
						}
					},
					{
						"name": "secrets",
						"map": {
							"computed_optional_required": "computed",
							"element_type": {
									"string": {}
							}
						}
					},
					{
						"name": "redirect_uris",
						"list": {
							"computed_optional_required": "optional",
							"element_type": {
									"string": {}
							}
						}
					},
					{
						"name": "post_logout_redirect_uris",
						"list": {
							"computed_optional_required": "optional",
							"element_type": {
									"string": {}
							}
						}
					},
					{
						"name": "cors_allowed_origins",
						"list": {
							"computed_optional_required": "optional",
							"element_type": {
									"string": {}
							}
						}
					}
				]
			}
		}
	],
	"version": "0.1"
}