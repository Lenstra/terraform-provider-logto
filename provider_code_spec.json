									"name": "length",
									"single_nested": {
										"attributes": [
											{
												"name": "max",
												"number": {
													"computed_optional_required": "computed_optional"
												}
											},
											{
												"name": "min",
												"number": {
													"computed_optional_required": "computed_optional"
